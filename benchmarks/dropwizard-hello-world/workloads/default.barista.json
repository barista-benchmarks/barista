{
    "endpoint": "http://127.0.0.1:8012/hello-world",
    "output_dir": "logs/",
    "vm_options": [],
    "app_args": ["server", "${BENCHMARK_HOME}/minimal.yml"],
    "load_testing":{
        "lua_script": "helloworld.lua",
        "connections": 1,
        "threads": 1,
        "startup":{
            "iterations": 10,
            "cmd_app_prefix": ["taskset", "-c", "0-3"]
        },
        "warmup":{
            "iterations": 9,
            "iteration_time_seconds": 10
        },
        "throughput":{
            "iterations": 1,
            "iteration_time_seconds": 60
        },
        "latency_measurement":{
            "iterations": 1,
            "iteration_time_seconds": 60,
            "search_strategy": "FIXED",
            "rates": 15000
        }
    }
}
